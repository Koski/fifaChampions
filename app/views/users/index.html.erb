<div class = "col-md-2">

  <h1>All users</h1>

  <table class= "table table-hover">
    <thead>
      <tr>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td> <%= link_to user.username, user %> <%= image_tag user.avatar.url(:thumb) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

</div>
