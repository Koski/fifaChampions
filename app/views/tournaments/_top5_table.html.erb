

<table class = "table table-hover table-bordered">
	<thead>
		<th></th>
		<th>Name</th>
		<th><%= state %></th>
	</thead>
	<tbody>
		<% users.each do |user| %>
			<tr>
				<td><%= image_tag user.avatar.url(:thumb) %></td>
				<td><%= link_to user.username, user %></td>
				<td><%= user.send(method) %></td>
			</tr>
		<% end %>
	</tbody>
</table>


